    <webLinks>
        <fullName>Sync_Achievements</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Sync Achievements</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}
var parentId; 
if(&quot;{!Contact.Id}&quot; != &quot;&quot;)
  parentId = &quot;{!Contact.Id}&quot;;
else
  parentId = &quot;{!Lead.Id}&quot;;
console.log(&apos;--------------&apos;+parentId);
var result = sforce.apex.execute(&quot;ForceSyncWebService&quot;,&quot;forceSync&quot;,{recordId : parentId});
if(result)
{
  alert(&apos;Successfully synchronized.&apos;);
  window.location.reload();
}
else
  alert(&apos;There might be some problem while synchronizing.&apos;);</url>
