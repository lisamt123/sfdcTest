    <validationRules>
        <fullName>Assigned_Hours_Must_Be_In_Quarter_Hours</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
  Assigned_Hours__c &lt; 1,
  AND(
    CONTAINS(TEXT(Assigned_Hours__c), &apos;.&apos;),  
    MID(TEXT(Assigned_Hours__c), FIND(&apos;.&apos;, TEXT(Assigned_Hours__c)) + 1, LEN(TEXT(Assigned_Hours__c))) != &apos;0&apos;,
    MID(TEXT(Assigned_Hours__c), FIND(&apos;.&apos;, TEXT(Assigned_Hours__c)) + 1, LEN(TEXT(Assigned_Hours__c))) != &apos;25&apos;,
    MID(TEXT(Assigned_Hours__c), FIND(&apos;.&apos;, TEXT(Assigned_Hours__c)) + 1, LEN(TEXT(Assigned_Hours__c))) != &apos;5&apos;,
    MID(TEXT(Assigned_Hours__c), FIND(&apos;.&apos;, TEXT(Assigned_Hours__c)) + 1, LEN(TEXT(Assigned_Hours__c))) != &apos;75&apos;
  )
)</errorConditionFormula>
        <errorDisplayField>Assigned_Hours__c</errorDisplayField>
        <errorMessage>Assigned Hours must be greater than 1, and in increments of quarter hours.</errorMessage>
    </validationRules>
