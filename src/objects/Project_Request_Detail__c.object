    <validationRules>
        <fullName>Assign_Allocation_Required_If_Fulfilled</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
AND( 
ISPICKVAL(Status__c, &quot;Closed&quot;), 
OR( 
ISPICKVAL(Status_Detail__c, &quot;Fulfilled&quot;), 
ISPICKVAL(Status_Detail__c, &quot;Partially Fulfilled&quot;) 
) 
), 
ISBLANK(Assigned_Allocation__c) 
)</errorConditionFormula>
        <errorDisplayField>Assigned_Allocation__c</errorDisplayField>
        <errorMessage>Assigned Allocation is required in order to fulfill a Project Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Assigned_End_Date_Required_If_Fulfilled</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  AND(
