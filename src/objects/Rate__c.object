    <validationRules>
        <fullName>Ensure_Rate_Specific_Dates_Are_A_Subset</fullName>
        <active>true</active>
        <description>Ensures that the Rate specific date range, if provided, is a subset of the Agreement date range</description>
        <errorConditionFormula>OR(
  Rate_Specific_Effective_Date__c &lt; Agreement__r.Effective_Date__c,
  Rate_Specific_Effective_Date__c &gt; Agreement__r.Valid_Through__c,
  Rate_Specific_Valid_Through__c &lt; Agreement__r.Effective_Date__c,
  Rate_Specific_Valid_Through__c &gt; Agreement__r.Valid_Through__c
)</errorConditionFormula>
        <errorMessage>If you specify a date range for this Rate, it must be a subset of the Agreement&apos;s date range.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rate_Specific_Dates_Are_All_Or_Nothing</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
  AND(
   ISBLANK(Rate_Specific_Effective_Date__c),
   NOT(ISBLANK(Rate_Specific_Valid_Through__c))
  ),
  AND(
   NOT(ISBLANK(Rate_Specific_Effective_Date__c)),
   ISBLANK(Rate_Specific_Valid_Through__c)
  )
)</errorConditionFormula>
        <errorMessage>If you&apos;re going to set a Rate specific date range, you must provide both an Effective Date and Valid Through date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Through_Must_Be_GT_Effective_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Valid_Through__c &lt;= Effective_Date__c</errorConditionFormula>
        <errorMessage>The Valid Through date must be greater than the Effective Date</errorMessage>
    </validationRules>
