    <validationRules>
        <fullName>Billable_Rate_Change_If_No_TimeEntries</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Total_Number_of_Hours__c &gt; 0,ISCHANGED( Rate__c ), false)</errorConditionFormula>
        <errorDisplayField>Rate__c</errorDisplayField>
        <errorMessage>Time has already been logged against Assignment, so you cannot change the Billable Rate. Please create a new Assignment instead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cost_Rate_Change_If_No_TimeEntries</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Total_Number_of_Hours__c &gt; 0,ISCHANGED( Cost_Rate__c ), false)</errorConditionFormula>
        <errorDisplayField>Cost_Rate__c</errorDisplayField>
        <errorMessage>Time has already been logged against Assignment, so you cannot change the Cost Rate. Please create a new Assignment instead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_is_later_than_the_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISBLANK(Start_Date__c)), 
NOT(ISBLANK(End_Date__c)), 
Start_Date__c &gt; End_Date__c
)</errorConditionFormula>
        <errorMessage>The Assignment&apos;s End Date should be later than Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_End_Date_Cannot_More_Than_Two_Year</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
  AND(
    NOT(ISBLANK(End_Date__c)),
    NOT(ISBLANK(Start_Date__c))
  ),
  End_Date__c - Start_Date__c &gt; 730,
  false
)</errorConditionFormula>
        <errorMessage>An Assignment can only be scheduled for a maximum of 2 years.</errorMessage>
    </validationRules>
